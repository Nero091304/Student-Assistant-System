#btnTest:hover{
    background-color: #804B18;
}
#btnCourse:hover{
    background-color: #1460AA;
}
#btnLSPU:hover{
    background-color: #A40000;
}
#btnRIASEC:hover{
    background-color: #249E95;
}
#btnHistory:hover{
    background-color: #80209E;
}

#btnTest:pressed{
    background-color: #553210;
}
#btnCourse:pressed{
