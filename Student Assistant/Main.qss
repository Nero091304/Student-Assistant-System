}

#btnUpload {
    background-color: transparent;
    border: transparent;
}

#btnUpload1, #btnSave {
    background-color: #8644F2;
    color: white;
    border-radius: 10px;
    padding: 0px;
    font-size: 20px;
    font-family: "Segoe UI", sans-serif;
    font-weight: 1500;
}

#btnRemove {
    background-color: #AA0000;
    color: white;
    border-radius: 10px;
    padding: 0px;
    font-size: 20px;
    font-family: "Segoe UI", sans-serif;
    font-weight: 1500;
}

#btnUpload:hover {
    background-color: #AAAAAA;
}

#btnUpload1:hover, #btnSave:hover {
     background-color: #857AA5;
}

#btnUpload1:pressed, #btnSave:pressed {
     background-color: #483878;
}

#btnRemove:hover{
    background-color: #FF0000;
}

#btnRemove:pressed{
    background-color: #840808;
}

#btnClose, #btnClose1, #btnMaximize, #btnMinimize {
    color: white;
    border-radius: 10px;
}

#btnClose:hover, #btnClose1:hover {
    background-color: #AA0000;
}
#btnClose:pressed, #btnClose1:pressed {
     background-color: #840808;
}
#btnMaximize:hover, #btnMinimize:hover{
    background-color: #4F4A5D;
}
#btnMaximize:pressed, #btnMinimize:pressed{
    background-color: #857AA5;
}

#lblUsername {
     color: #FFFFFF;
     font-size: 42px;
     font-family: "Segoe UI", sans-serif;
     font-weight: 1500;
}
#lblSAS
{
    color: #FFFFFF;
    font-size: 60px;
    font-family: "Georgia";
    font-weight: 1500;
}   
#lblSAS:hover {
     color: #8644f2;
}
#lblSAS:pressed {
     color: #AA7700;
}

#btnback {
    background-color: transparent;
    color: white;
    border-radius: 10px;
    font-size: 20px;
    font-family: "Consolas";
    font-weight: bold;
}

#btnback:hover {
    background-color: #ADABB1;
    background-color: rgba(173, 171, 177, 0.5);
}

